{
	"name": "slm-dashboard",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"electron": "^6.0.7",
		"electron-is-dev": "^1.1.0",
		"eslint": "^6.3.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.3",
		"node-sass": "^4.12.0",
		"prettier": "^1.18.2",
		"react": "^16.9.0",
		"react-bootstrap": "^1.0.0-beta.12",
		"react-dom": "^16.9.0",
		"react-scripts": "3.1.1"
	},
	"scripts": {
